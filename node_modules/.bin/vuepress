#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules/vuepress/bin/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules/vuepress/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules/vuepress/bin/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules/vuepress/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-beta.62_@vuepress+client@2.0.0-beta.62_vue@3.3.2/node_modules:/mnt/d/尚硅谷前端/项目源码/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
